{
    "manifest_version": 3,
    "name": "Tariq Extractor",
    "version": "1.0",
    "description": "This extension made by: x.com/its1gg .",
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
